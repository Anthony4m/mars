func twosum(nums: []int, target: int) -> []int {
    for mut i := 0; i < len(nums); i = i + 1 {
        for mut j := i + 1; j < len(nums); j = j + 1 {
            if nums[j] == target - nums[i] {
                return [i, j];
            }
        }
    }
    return [];
}

func main() {
    nums := [2, 7, 11, 15];
    target := 9;

    result := twosum(nums, target);
    println(result);
} 