func test_array_param(nums : []int) -> int {
    x := nums[0];
    return x;
}

func test_array_param_in_if(nums : []int) -> int {
    if nums[0] == 2 {
        return 1;
    }
    return 0;
}

func main() {
    nums := [2, 7, 11, 15];
    
    result1 := test_array_param(nums);
    println("Array param: ");
    println(result1);
    
    result2 := test_array_param_in_if(nums);
    println("Array param in if: ");
    println(result2);
} 