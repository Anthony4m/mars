func two_sum(nums: []int, target: int) -> []int {
    for i := 0; i < len(nums); i = i + 1 {
        for j := i + 1; j < len(nums); j = j + 1 {
            if nums[i] + nums[j] == target {
                return [i, j];
            }
        }
    }
    return [-1, -1];
}

func main() {
    nums := [2, 7, 11, 15];
    target := 9;
    result := two_sum(nums, target);
    println(result);
} 