func test_int_return(nums : []int) -> int {
    for mut i := 0; i < 4; i = i + 1 {
        if nums[i] == 2 {
            return i;
        }
    }
    return -1;
}

func test_array_return(nums : []int) -> []int {
    for mut i := 0; i < 4; i = i + 1 {
        if nums[i] == 2 {
            return [i, 0];
        }
    }
    return [-1, -1];
}

func main() {
    nums := [2, 7, 11, 15];
    
    result1 := test_int_return(nums);
    println("Int return: ");
    println(result1);
    
    result2 := test_array_return(nums);
    println("Array return: ");
    println(result2);
} 