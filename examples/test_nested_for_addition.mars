func test_nested_for_simple(nums : []int) -> []int {
    for mut i := 0; i < 4; i = i + 1 {
        for mut j := i + 1; j < 4; j = j + 1 {
            if nums[i] == 2 {
                return [i, j];
            }
        }
    }
    return [-1, -1];
}

func test_nested_for_addition(nums : []int) -> []int {
    for mut i := 0; i < 4; i = i + 1 {
        for mut j := i + 1; j < 4; j = j + 1 {
            if nums[i] + nums[j] == 9 {
                return [i, j];
            }
        }
    }
    return [-1, -1];
}

func main() {
    nums := [2, 7, 11, 15];
    
    result1 := test_nested_for_simple(nums);
    println("Nested for simple: ");
    println(result1);
    
    result2 := test_nested_for_addition(nums);
    println("Nested for addition: ");
    println(result2);
} 