func test_single_loop(nums : []int, target : int) -> []int {
    for i := 0; i < 4; i = i + 1 {
        if nums[i] == target {
            return [i, 0];
        }
    }
    return [-1, -1];
}

func test_nested_loops(nums : []int, target : int) -> []int {
    for i := 0; i < 4; i = i + 1 {
        for j := i + 1; j < 4; j = j + 1 {
            if nums[i] == target {
                return [i, j];
            }
        }
    }
    return [-1, -1];
}

func main() {
    nums := [2, 7, 11, 15];
    target := 2;
    
    result1 := test_single_loop(nums, target);
    println("Single loop: ");
    println(result1);
    
    result2 := test_nested_loops(nums, target);
    println("Nested loops: ");
    println(result2);
} 