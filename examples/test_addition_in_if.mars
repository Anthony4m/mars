func test_addition_in_if(nums : []int) -> []int {
    if nums[0] + nums[1] == 9 {
        return [0, 1];
    }
    return [-1, -1];
}

func test_simple_equality(nums : []int) -> []int {
    if nums[0] == 2 {
        return [0, 0];
    }
    return [-1, -1];
}

func main() {
    nums := [2, 7, 11, 15];
    
    result1 := test_simple_equality(nums);
    println("Simple equality: ");
    println(result1);
    
    result2 := test_addition_in_if(nums);
    println("Addition in if: ");
    println(result2);
} 